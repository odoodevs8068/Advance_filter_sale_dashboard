<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="search_record_tree_view" model="ir.ui.view">
         <field name="name">search.record.tree</field>
        <field name="model">search.record</field>
        <field name="arch" type="xml">
            <tree delete="1" editable='top' create="1">
                <field name="models"/>
                <field name="name"/>
                <field name="model_name"/>
            </tree>
        </field>
    </record>

    <record id="action_search_record" model="ir.actions.act_window">
        <field name="name">Seach Record</field>
        <field name="res_model">search.record</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="search_record_tree_view"/>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Configure To Search Records!
            </p>
        </field>
    </record>

    <record id="product_template_form_inherit" model="ir.ui.view">
        <field name="name">Product Template</field>
        <field name="model">Product.Template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='general_information']/group[1]/group[1]/field[@name='uom_po_id']" position="after">
                <field name="density"/>
            </xpath>
            <xpath expr="//page[@name='general_information']/group[1]/group[1]/field[@name='uom_po_id']" position="after">
                <field name="density"/>
            </xpath>
        </field>
    </record>
