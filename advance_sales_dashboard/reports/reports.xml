<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="report_sales_dynamic" model="ir.actions.report">
        <field name="name">Sales Report</field>
        <field name="model">sale.order</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">advance_sales_dashboard.report_sales_dynamic_order_view</field>
        <field name="report_file">advance_sales_dashboard.report_sales_dynamic_order_view</field>
        <field name="binding_type">report</field>
        <field name="binding_model_id" eval="False"/>
    </record>

    <record id="report_sales_dashboard" model="ir.actions.report">
        <field name="name">Sales Dashboard PDF Report</field>
        <field name="model">ale_custom.dashboard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">advance_sales_dashboard.sale_order_report</field>
        <field name="report_file">advance_sales_dashboard.sale_order_report</field>
        <field name="binding_type">report</field>
        <field name="binding_model_id" eval="False"/>
    </record>



</odoo>